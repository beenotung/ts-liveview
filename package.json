{
  "name": "ts-liveview",
  "version": "1.1.0",
  "description": "LiveView enables rich, real-time user experiences with server-rendered HTML",
  "keywords": [
    "liveview",
    "SSR",
    "SPA",
    "PWA",
    "web",
    "s-js",
    "typescript"
  ],
  "author": "Beeno Tung <aabbcc1241@yahoo.com.hk> (https://beeno-tung.surge.sh)",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "postinstall": "rimraf node_modules/@beenotung/tslib/index.d.ts",
    "watch:tsc": "tsc -w",
    "watch:js": "run-s build:demo watch:demo",
    "watch:demo": "nodemon --watch dist -x run-p demo:*",
    "watch:test": "ts-mocha --watch \"src/**/*.spec.ts\"",
    "demo:spa": "node dist/examples/spa-demo/main",
    "demo:chatroom": "node dist/examples/chatroom-demo/main",
    "dev": "run-p watch:tsc watch:test",
    "dev:demo": "run-p watch:tsc watch:js",
    "test": "time ts-mocha \"src/**/*.spec.ts\"",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"examples/**/*.ts\" ",
    "postformat": "tslint -p . --fix",
    "gen-index": "gen-index --single-quote --no-semi --excludeDir test",
    "postgen-index": "sh scripts/sort-index",
    "prebuild": "run-s clean gen-index",
    "build": "run-p format build:lib",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:demo": "tsc",
    "prepublishOnly": "run-s test build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "browserify": "^16.5.0",
    "debug": "^4.1.1",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "morphdom": "^2.5.12",
    "quick-hash": "^1.0.4",
    "route-node": "^4.1.1",
    "s-js": "^0.4.9",
    "sanitize-html": "^2.3.0",
    "tslib": "^1.11.1",
    "typestub-primus": "^1.3.2",
    "url-router.ts": "^0.2.1"
  },
  "devDependencies": {
    "@beenotung/gen-index": "^1.2.1",
    "@beenotung/tslib": "^16.30.0",
    "@stencil/core": "^1.12.7",
    "@types/body-parser": "^1.19.0",
    "@types/browserify": "^12.0.36",
    "@types/chai": "^4.2.14",
    "@types/compression": "^1.7.0",
    "@types/debug": "^4.1.5",
    "@types/escape-html": "^1.0.0",
    "@types/express": "^4.17.3",
    "@types/express-minify": "^0.1.34",
    "@types/express-serve-static-core": "^4.17.2",
    "@types/express-session": "^1.17.3",
    "@types/mocha": "^8.2.0",
    "@types/node": "*",
    "@types/quick-hash": "^1.0.0",
    "@types/react": "^16.9.35",
    "@types/sanitize-html": "^1.27.0",
    "@types/serve-static": "^1.13.3",
    "@types/sinon": "^9.0.9",
    "@types/ws": "^7.2.3",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "express-minify": "^1.0.0",
    "husky": "^4.2.3",
    "minify": "^5.1.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "s-array": "^0.4.10",
    "sinon": "^9.2.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.1.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.10.1",
    "typescript": "^3.8.3",
    "url-pattern": "^1.0.3",
    "ws": "^7.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/ts-liveview.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/ts-liveview/issues"
  },
  "homepage": "https://github.com/beenotung/ts-liveview#readme"
}
